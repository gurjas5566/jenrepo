pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                javac samplepro.java
                echo 'Build in Progress'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                java samplepro
                echo 'Deploying'
            }
        }
    }
}
